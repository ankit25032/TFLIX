as normal.\nregisterValidSW(swUrl);}}).catch(function(){console.log('No internet connection found. App is running in offline mode.');});}function unregister(){if('serviceWorker'in navigator){navigator.serviceWorker.ready.then(function(registration){registration.unregister();});}}\n// CONCATENATED MODULE: ./src/index.js\nreact_dom_default.a.render(/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(src_App_0,{}),document.getElementById(\"root\"));register();\n\n/***/ })\n\n},[[78,1,2]]]);","inputSourceMap":{"version":3,"sources":["D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\mini-css-extract-plugin\\dist\\loader.js??ref--5-oneOf-4-0!D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\css-loader\\dist\\cjs.js??ref--5-oneOf-4-1!D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\postcss-loader\\src\\index.js??postcss!D:\\VS_Code\\RECAT\\NETFLIX\\src\\components\\movies.css","D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\mini-css-extract-plugin\\dist\\loader.js??ref--5-oneOf-4-0!D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\css-loader\\dist\\cjs.js??ref--5-oneOf-4-1!D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\postcss-loader\\src\\index.js??postcss!D:\\VS_Code\\RECAT\\NETFLIX\\src\\pages\\Home.css","D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\mini-css-extract-plugin\\dist\\loader.js??ref--5-oneOf-4-0!D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\css-loader\\dist\\cjs.js??ref--5-oneOf-4-1!D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\postcss-loader\\src\\index.js??postcss!D:\\VS_Code\\RECAT\\NETFLIX\\src\\pages\\main.css","D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\mini-css-extract-plugin\\dist\\loader.js??ref--5-oneOf-4-0!D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\css-loader\\dist\\cjs.js??ref--5-oneOf-4-1!D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\postcss-loader\\src\\index.js??postcss!D:\\VS_Code\\RECAT\\NETFLIX\\src\\index.css","D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\mini-css-extract-plugin\\dist\\loader.js??ref--5-oneOf-4-0!D:\\VS_Code\\RECAT\\NETFLIX\\node_modules\\css-loader\\dist\\cjs.js??ref--5-oneOf-4